{
  "name": "babel-template-routex",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "compile": "npx babel src --out-dir routes",
    "dev": "concurrently 'npm run compile -- --watch' 'npm run dev:server'",
    "dev:server": "nodemon --exec 'npx routex -d routes'",
    "start": "npx routex -d routes"
  },
  "dependencies": {
    "routex": "github:barelyhuman/routex"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "concurrently": "^5.2.0"
  }
}
